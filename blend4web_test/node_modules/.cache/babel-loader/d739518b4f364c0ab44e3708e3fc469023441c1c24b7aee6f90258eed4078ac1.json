{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      selectedImage: null,\n      images: [],\n      samplingSteps: 20,\n      cfgScale: 10,\n      denoisingStrength: 0.30,\n      imageDialog: false,\n      currentImage: '',\n      compareImage: null,\n      clipPosition: 50\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        const reader = new FileReader();\n        reader.onload = () => {\n          this.selectedImage = reader.result;\n          this.images = [];\n        };\n        reader.readAsDataURL(file);\n      }\n    },\n    applyChanges() {\n      if (!this.selectedImage) {\n        console.error('Изображение не выбрано');\n        return;\n      }\n      const param2Value = this.samplingSteps.toString();\n      const param3Value = this.cfgScale.toString();\n      const param4Value = this.denoisingStrength.toString();\n      const base64Image = this.selectedImage.split(',')[1];\n      axios.post('http://127.0.0.1:5000/api/custom_task', {\n        param1: base64Image,\n        param2: param2Value,\n        param3: param3Value,\n        param4: param4Value\n      }, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(response => {\n        if (response.data.images && response.data.images.length > 0) {\n          this.images = response.data.images.map(img => `data:image/png;base64,${img}`);\n        } else {\n          console.error('Ответ сервера не содержит изображения');\n        }\n      }).catch(error => {\n        console.error('Ошибка при запросе к серверу:', error);\n      });\n    },\n    openImage(imageSrc) {\n      this.currentImage = imageSrc;\n      this.compareImage = this.selectedImage;\n      this.imageDialog = true;\n    },\n    closeImage() {\n      this.imageDialog = false;\n    },\n    handleMouseMove(event) {\n      const {\n        left,\n        width\n      } = this.$refs.comparisonContainer.getBoundingClientRect();\n      const xPosition = event.clientX - left;\n      const clipPercent = xPosition / width * 100;\n      this.clipPosition = Math.max(0, Math.min(100, clipPercent));\n    },\n    resetComparison() {\n      this.clipPosition = 50;\n    }\n  }\n};","map":{"version":3,"names":["axios","data","selectedImage","images","samplingSteps","cfgScale","denoisingStrength","imageDialog","currentImage","compareImage","clipPosition","methods","handleFileUpload","event","file","target","files","reader","FileReader","onload","result","readAsDataURL","applyChanges","console","error","param2Value","toString","param3Value","param4Value","base64Image","split","post","param1","param2","param3","param4","headers","then","response","length","map","img","catch","openImage","imageSrc","closeImage","handleMouseMove","left","width","$refs","comparisonContainer","getBoundingClientRect","xPosition","clientX","clipPercent","Math","max","min","resetComparison"],"sources":["C:\\Users\\golov\\OneDrive\\Рабочий стол\\projectTest\\blend4web_test\\src\\App.vue"],"sourcesContent":["<template>\n  <v-app>\n    <v-container fluid>\n      <v-file-input label=\"Выбрать файл\" @change=\"handleFileUpload\" class=\"file-input\"></v-file-input>\n      \n      <v-img v-if=\"selectedImage\" :src=\"selectedImage\" width=\"300\" height=\"200\" class=\"uploaded-image\"></v-img>\n      \n      <v-slider v-model=\"samplingSteps\" :label=\"`Sampling Steps: ${samplingSteps}`\" min=\"1\" max=\"100\" step=\"1\" thumb-label=\"always\" class=\"my-5\"></v-slider>\n      <v-slider v-model=\"cfgScale\" :label=\"`CFG Scale: ${cfgScale}`\" min=\"1\" max=\"30\" step=\"1\" thumb-label=\"always\" class=\"my-5\"></v-slider>\n      <v-slider v-model=\"denoisingStrength\" :label=\"`Denoising Strength: ${denoisingStrength}`\" min=\"0\" max=\"1\" step=\"0.01\" thumb-label=\"always\" class=\"my-5\"></v-slider>\n      \n      <v-btn color=\"primary\" @click=\"applyChanges\" class=\"apply-button\">Применить</v-btn>\n      \n      <div v-if=\"images.length\" class=\"results-section\">\n        <v-subheader>Результат</v-subheader>\n        <v-row>\n          <v-col v-for=\"(image, index) in images\" :key=\"'received-' + index\" cols=\"12\" sm=\"4\" md=\"3\" class=\"tight-spacing\">\n            <v-img :src=\"image\" height=\"200\" class=\"result-image\" @click=\"openImage(image)\"></v-img>\n            <div class=\"d-flex justify-center mt-2\">\n              <v-btn small color=\"primary\" :href=\"image\" :download=\"'image' + index + '.png'\" v-if=\"images.length\">Скачать</v-btn>\n            </div>\n          </v-col>\n        </v-row>\n      </div>\n    </v-container>\n\n    <v-dialog v-model=\"imageDialog\" persistent max-width=\"600px\">\n      <v-card>\n        <v-toolbar dense flat>\n          <v-spacer></v-spacer>\n          <v-btn icon @click=\"closeImage\">\n            <v-icon>mdi-close</v-icon>\n          </v-btn>\n        </v-toolbar>\n        <v-container fluid>\n          <div class=\"comparison-container\" @mousemove=\"handleMouseMove\" @mouseleave=\"resetComparison\" ref=\"comparisonContainer\">\n            <v-img :src=\"compareImage\" class=\"absolute-image\"></v-img>\n            <v-img :src=\"currentImage\" class=\"slider-image\" :style=\"{ clipPath: `polygon(0 0, ${clipPosition}% 100%, 0% 100%, 0 0)` }\"></v-img>\n          </div>\n        </v-container>\n      </v-card>\n    </v-dialog>\n  </v-app>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      selectedImage: null,\n      images: [],\n      samplingSteps: 20,\n      cfgScale: 10,\n      denoisingStrength: 0.30,\n      imageDialog: false,\n      currentImage: '',\n      compareImage: null,\n      clipPosition: 50,\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        const reader = new FileReader();\n        reader.onload = () => {\n          this.selectedImage = reader.result;\n          this.images = [];\n        };\n        reader.readAsDataURL(file);\n      }\n    },\n    applyChanges() {\n      if (!this.selectedImage) {\n        console.error('Изображение не выбрано');\n        return;\n      }\n\n      const param2Value = this.samplingSteps.toString();\n      const param3Value = this.cfgScale.toString();\n      const param4Value = this.denoisingStrength.toString();\n      const base64Image = this.selectedImage.split(',')[1];\n\n      axios.post('http://127.0.0.1:5000/api/custom_task', {\n        param1: base64Image,\n        param2: param2Value,\n        param3: param3Value,\n        param4: param4Value,\n      }, {\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      })\n      .then(response => {\n        if (response.data.images && response.data.images.length > 0) {\n          this.images = response.data.images.map(img => `data:image/png;base64,${img}`);\n        } else {\n          console.error('Ответ сервера не содержит изображения');\n        }\n      })\n      .catch(error => {\n        console.error('Ошибка при запросе к серверу:', error);\n      });\n    },\n    openImage(imageSrc) {\n      this.currentImage = imageSrc;\n      this.compareImage = this.selectedImage;\n      this.imageDialog = true;\n    },\n    closeImage() {\n      this.imageDialog = false;\n    },\n    handleMouseMove(event) {\n      const { left, width } = this.$refs.comparisonContainer.getBoundingClientRect();\n      const xPosition = event.clientX - left;\n      const clipPercent = (xPosition / width) * 100;\n      this.clipPosition = Math.max(0, Math.min(100, clipPercent));\n    },\n    resetComparison() {\n      this.clipPosition = 50;\n    },\n  }\n};\n</script>\n\n<style>\n.file-input, .my-5 {\n  margin-bottom: 20px;\n}\n\n.uploaded-image, .result-image {\n  margin-top: 20px;\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\n}\n\n.apply-button, .results-section {\n  margin-top: 20px;\n}\n\n.result-image:hover {\n  cursor: pointer;\n  transform: scale(1.05);\n  box-shadow: 0 0 8px rgba(0,0,0,0.2);\n}\n\n.mt-2 {\n  margin-top: 8px;\n}\n\n.comparison-container {\n  position: relative;\n  width: 100%;\n  height: 400px;\n}\n\n.absolute-image, .slider-image {\n  position: absolute;\n  width: 100%;\n  height: 100%;\n}\n\n.slider-image {\n  transition: clip-path 0.2s ease-out;\n}\n</style>\n"],"mappings":"AA8CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,IAAI;MACnBC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,EAAE;MACZC,iBAAiB,EAAE,IAAI;MACvBC,WAAW,EAAE,KAAK;MAClBC,YAAY,EAAE,EAAE;MAChBC,YAAY,EAAE,IAAI;MAClBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,MAAMG,MAAK,GAAI,IAAIC,UAAU,CAAC,CAAC;QAC/BD,MAAM,CAACE,MAAK,GAAI,MAAM;UACpB,IAAI,CAACjB,aAAY,GAAIe,MAAM,CAACG,MAAM;UAClC,IAAI,CAACjB,MAAK,GAAI,EAAE;QAClB,CAAC;QACDc,MAAM,CAACI,aAAa,CAACP,IAAI,CAAC;MAC5B;IACF,CAAC;IACDQ,YAAYA,CAAA,EAAG;MACb,IAAI,CAAC,IAAI,CAACpB,aAAa,EAAE;QACvBqB,OAAO,CAACC,KAAK,CAAC,wBAAwB,CAAC;QACvC;MACF;MAEA,MAAMC,WAAU,GAAI,IAAI,CAACrB,aAAa,CAACsB,QAAQ,CAAC,CAAC;MACjD,MAAMC,WAAU,GAAI,IAAI,CAACtB,QAAQ,CAACqB,QAAQ,CAAC,CAAC;MAC5C,MAAME,WAAU,GAAI,IAAI,CAACtB,iBAAiB,CAACoB,QAAQ,CAAC,CAAC;MACrD,MAAMG,WAAU,GAAI,IAAI,CAAC3B,aAAa,CAAC4B,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC;MAEpD9B,KAAK,CAAC+B,IAAI,CAAC,uCAAuC,EAAE;QAClDC,MAAM,EAAEH,WAAW;QACnBI,MAAM,EAAER,WAAW;QACnBS,MAAM,EAAEP,WAAW;QACnBQ,MAAM,EAAEP;MACV,CAAC,EAAE;QACDQ,OAAO,EAAE;UACP,cAAc,EAAE;QAClB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACrC,IAAI,CAACE,MAAK,IAAKmC,QAAQ,CAACrC,IAAI,CAACE,MAAM,CAACoC,MAAK,GAAI,CAAC,EAAE;UAC3D,IAAI,CAACpC,MAAK,GAAImC,QAAQ,CAACrC,IAAI,CAACE,MAAM,CAACqC,GAAG,CAACC,GAAE,IAAM,yBAAwBA,GAAI,EAAC,CAAC;QAC/E,OAAO;UACLlB,OAAO,CAACC,KAAK,CAAC,uCAAuC,CAAC;QACxD;MACF,CAAC,EACAkB,KAAK,CAAClB,KAAI,IAAK;QACdD,OAAO,CAACC,KAAK,CAAC,+BAA+B,EAAEA,KAAK,